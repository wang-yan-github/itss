/* vant样式优化 */
.box .van-cell__title{font-weight:600;position:relative;font-size:32rpx;}
.box .van-cell__title:before{content:"";width:8rpx;height:20rpx;background:#4478fe;position:absolute;left:-30rpx;top:10rpx;}

.tabs-panel{height:100rpx;display:flex;flex-direction: row;width:100%;background:#fff;position:fixed;top:0;left:0;z-index:11;}
.tabs-panel .li{line-height:100rpx;text-align:center;flex:1;font-size:28rpx;position:relative;}
.tabs-panel .li.active{color:#4478fe;font-weight:bold;}
.tabs-panel .li.active:after{content:"";position:absolute;bottom:2px;width:10%;left:45%;height:8rpx;background:#4478fe;border-radius:20rpx;}
.tabs-content{width:92%;margin-top:40rpx;display:none;position:relative;z-index:1;}
.tabs-content.active{display:block;}
.tabs-content .li{margin-bottom:30rpx;background:#fff;box-shadow: 0 0 20rpx rgba(0,0,0,0);padding:30rpx;box-sizing:border-box;border-radius:10px;}
.tabs-content .li .row1{line-height:40rpx;font-size:30rpx;display:flex;justify-content: space-between;padding: 10rpx 0;}
.tabs-content .li .row1 .ordernum{color:#333;font-size:32rpx;flex: 1;width:80%;display:flex;}
.tabs-content .li .row1 .ordernum .words{word-break: break-all;overflow: hidden;  display: -webkit-box;  -webkit-box-orient: vertical;  -webkit-line-clamp: 1;}
.tabs-content .li .row1 .ordernum .iconfont{font-size:36rpx;display:inline-block;margin-right:16rpx;} 
.tabs-content .li .row1 .state{color:#f9882e;margin-left: 16rpx;}
.tabs-content .li .row2{line-height:50rpx;margin-top:20rpx;}
.tabs-content .li .row2 .item-row{line-height:40rpx;font-size:28rpx;display:flex;flex-direction: row;}
.tabs-content .li .row2 .p1{color:#999;display:block;width:180rpx;}
.tabs-content .li .row2 .p2{color:#666;display:block;width:480rpx;}
.tabs-content .li .row2 .p3{color:#999;display:block;width:170rpx;margin:0;padding:0;}
.tabs-content .li .row2 .p4{color:#999;line-height:40rpx;}
.tabs-content .li .row2 .piclist{margin-top:10rpx;display:flex;flex-direction: row;}
.tabs-content .li .row2 .piclist .li{width:160rpx;height:120rpx;margin:0;padding:0;margin-right:10rpx;}
.tabs-content .li .row2 .piclist .li image{width:100%;height:100%;object-fit: cover;}
.tabs-content .li .row3{margin-top:20rpx;text-align: right;color:#ff0000;}
.tabs-content .li .row4{margin-top:20rpx;text-align: right;}
.tabs-content .li .row4 van-button{margin-left:5px;}
.tabs-content .li .row4 .btns{border-radius:40rpx;display:inline-block;padding:0 30rpx;background:#fff;border:1px solid #dadada;line-height:56rpx;color:#999;margin:0 5px;box-sizing: border-box;font-size:24rpx;}
.tabs-content .li .row4 .btns.jiaofei{color:#fff;background:#f9882e;border:1px solid #f9882e;}

.tabs-content .li .gou{display:none;position:absolute;bottom:-6rpx;right:4rpx;color:#fff;z-index:2;font-size:30rpx;}
.tabs-content .li.active{background:#eef6fe;border:1px solid #1989fa;position:relative;overflow:hidden;}
.tabs-content .li.active .ordernum{color:#1989fa;}
.tabs-content .li.active .p1{color:#1989fa;opacity:.8;}
.tabs-content .li.active .p2{color:#1989fa;}
.tabs-content .li.active .gou{display:block;}
.tabs-content .li.active:after{content:'';position:absolute;right:-40rpx;bottom:-40rpx;width: 0; height: 0;  border:40rpx solid transparent; border-left-color:#1989fa;transform: rotate(45deg);z-index: 1;}

