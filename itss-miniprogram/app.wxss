/**app.wxss**/
@import "fonts/iconfont.wxss";

/* 头部导航样式 */
.navigation-container {
  position: fixed;
  width: 100%;
  z-index: 99;
  top: 0;
  left: 0;
  background-color: transparent;
}
.navigation-bar {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.navigation-buttons {
  display: flex;
  align-items: center;
  margin-left: 4vw;
  /* border: 1px solid rgba(255, 255, 255, 0.05); */
  box-sizing: border-box;
  border-radius: 15px;
  background-color: transparent;
  color:#fff;
  text-align:center;
}
.navigation-buttons .iconfont{width:16px;height:16px;font-size:16px;}
.nav-img{
  height: 16px;
  width: 16px;
}
.navigation-title {
  position: absolute;
  left: 104px;
  right: 104px;
  text-align: center;
  font-size: 16px;
  font-weight: bold;
  color: #fff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 头部导航样式结束 */

